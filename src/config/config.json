{
  "public": {
    "enabled": false,
    "key": "/cdr/cert/wss.pem",
    "cert": "/cdr/cert/wss.pem",
    "host": "0.0.0.0",
    "port": 10023
  },
  "private": {
    "host": "0.0.0.0",
    "port": 10021
  },
  "mongodb": {
    "uri": "mongodb://pre.webitel.com:27017/webitel",
    "collectionCDR": "cdr",
    "collectionFile": "cdrFile",
    "collectionAuth": "authToken",
    "collectionEmail": "emailConfig",
    "collectionDomain": "domains",
    "collectionMedia": "mediaFile",
    "collectionAgentStatus": "agentStatus"
  },
  "replica": ["http://pre.webitel.com/cdr/"],
  "application": {
    "loglevel": "trace",
    "auth": {
      "tokenSecretKey": "myDevKey",
      "expiresDays": 7
    }
  },
  "recordFile": {
    "defaultProvider": "local",
    "maskPath": "$DOMAIN/$Y/$M/$D/$H",

    "providers": {
      "b2": {
        "accountId" : "xx",
        "bucketId" : "xx",
        "applicationKey" : "xx",
        "bucketName" : "xx"
      },
      "local": {
        "fileRoot": "/tmp/"
      }
    }
  },
  "mediaFile": {
    "maxFieldsSizeMB": 5,
    "defaultProvider": "local",
    "maskPath": "/media/$DOMAIN",

    "providers": {
      "b2": {
        "accountId" : "xx",
        "bucketId" : "xx",
        "applicationKey" : "xx",
        "bucketName" : "xx"
      },
      "local": {
        "fileRoot": "/tmp/"
      }
    }
  },
  "uploadAcl": {
    "mode": "allow",
    "ip": "0.0.0.0/0"
  }
}